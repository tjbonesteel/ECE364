# This script adds color and fonts to
# the window

from Tkinter import *
import sys

Root = Tk()
Root.title(sys.argv[0])

My_Label = Label(Root, text = '1C: Hello EE 364 Students',
                       fg="Red", # foreground color
                       bg="Light blue", # background color
                       font=("Helvetica", 24, "italic"))
My_Label.pack()
Root.mainloop()

